<?xml version="1.0" encoding="UTF-8"?>
        <StyledLayerDescriptor xmlns="http://www.opengis.net/sld"
          xmlns:ogc="http://www.opengis.net/ogc"
          xmlns:xlink="http://www.w3.org/1999/xlink"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.opengis.net/sld
         http://schemas.opengis.net/sld/1.0.0/StyledLayerDescriptor.xsd" version="1.0.0">
          <NamedLayer>
            <Name>depth_poly_light-25</Name>
            <UserStyle>
              <FeatureTypeStyle>
                <Title>sea_depth</Title>
          

                
                <Rule>
                    <ogc:Filter>
                        <ogc:PropertyIsEqualTo>
                            <ogc:PropertyName>z</ogc:PropertyName>
                            <ogc:Literal>-25</ogc:Literal>
                        </ogc:PropertyIsEqualTo>
                    </ogc:Filter>
                
                
                    <PolygonSymbolizer>
                        <Fill>
                          <CssParameter name="fill">#2494f0</CssParameter>
                        </Fill>

                    </PolygonSymbolizer>
                </Rule>
              </FeatureTypeStyle>
            </UserStyle>
          </NamedLayer>
        </StyledLayerDescriptor>
                